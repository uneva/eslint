{
    "name": "@uneva/eslint",
    "type": "module",
    "version": "1.0.12",
    "description": "Flat ESLint Configs",
    "author": "MaggotCode <udp.pkg.loss@gmail.com> (https://github.com/maggot-code)",
    "homepage": "https://github.com/uneva/eslint",
    "license": "MIT",
    "keywords": [
        "eslint",
        "eslint-config"
    ],
    "files": [
        "dist"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "esno": "esno",
        "build": "tsup",
        "dev": "tsup --watch & eslint --inspect-config",
        "watch": "tsup --watch",
        "preview": "eslint --inspect-config",
        "lint": "eslint .",
        "lint:fix": "pnpm run lint --fix",
        "release": "bumpp && pnpm publish",
        "test": "vitest",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "@eslint/config-inspector": "^0.4.7",
        "@types/eslint": "^8.56.10",
        "@types/node": "^20.12.7",
        "bumpp": "^9.4.0",
        "eslint": "^9.1.1",
        "esno": "^4.7.0",
        "tsup": "^8.0.2",
        "typescript": "^5.4.5",
        "vitest": "^1.5.1"
    },
    "dependencies": {
        "@stylistic/eslint-plugin": "^1.7.2",
        "@unocss/eslint-plugin": "^0.59.4",
        "eslint-define-config": "^2.1.0",
        "eslint-plugin-jsonc": "^2.15.1",
        "eslint-plugin-unused-imports": "^3.1.0",
        "eslint-plugin-vue": "^9.25.0",
        "globals": "^15.0.0",
        "jsonc-eslint-parser": "^2.4.0",
        "local-pkg": "^0.5.0",
        "typescript-eslint": "^7.7.1",
        "vue-eslint-parser": "^9.4.2"
    }
}
