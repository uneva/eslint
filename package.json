{
    "name": "@uneva/eslint",
    "type": "module",
    "version": "1.0.0",
    "description": "Flat ESLint Configs",
    "author": "MaggotCode <udp.pkg.loss@gmail.com> (https://github.com/maggot-code)",
    "homepage": "https://github.com/uneva/eslint",
    "license": "MIT",
    "keywords": [
        "eslint",
        "eslint-config"
    ],
    "files": [
        "dist"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch & eslint-flat-config-viewer",
        "watch": "tsup --watch",
        "view": "eslint-flat-config-viewer",
        "lint": "eslint .",
        "lint:fix": "pnpm run lint --fix",
        "release": "bumpp && pnpm publish",
        "test": "vitest",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "@types/eslint": "^8.56.5",
        "@types/node": "^20.11.28",
        "bumpp": "^9.4.0",
        "eslint": "^8.57.0",
        "eslint-flat-config-viewer": "^0.1.14",
        "esno": "^4.7.0",
        "tsup": "^8.0.2",
        "typescript": "^5.4.2",
        "vitest": "^1.4.0"
    },
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^7.3.1",
        "@typescript-eslint/parser": "^7.3.1",
        "@unocss/eslint-plugin": "^0.58.6",
        "eslint-define-config": "^2.1.0",
        "eslint-plugin-unused-imports": "^3.1.0",
        "eslint-plugin-vue": "^9.23.0",
        "globals": "^14.0.0",
        "local-pkg": "^0.5.0",
        "typescript-eslint": "^7.3.1",
        "vue-eslint-parser": "^9.4.2"
    }
}